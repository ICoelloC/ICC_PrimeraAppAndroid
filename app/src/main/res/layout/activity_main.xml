<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:background="#FFFFFF">


    <ImageButton
            android:layout_width="105dp"
            android:layout_height="119dp" app:srcCompat="@mipmap/notas" android:id="@+id/mainImageButtonNotes"
            android:scaleType="fitCenter"
            android:backgroundTint="#FFFFFF"
            android:contentDescription="@string/iconoMainLogoNotas"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="106dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="57dp" android:layout_marginEnd="249dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="506dp"/>
    <ImageButton
            android:layout_width="103dp"
            android:layout_height="117dp" app:srcCompat="@mipmap/contactos" android:id="@+id/mainImageButtonContacts"
            android:scaleType="fitCenter"
            android:backgroundTint="#FFFFFF" android:layout_marginEnd="56dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="108dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="253dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="506dp"
            android:contentDescription="@string/mainImageButtonContacts"/>
</androidx.constraintlayout.widget.ConstraintLayout>